programa {
  funcao inicio() {
    
    inteiro num1, num2
  
    
  escreva("\nDigite um número: ")
  leia(num1)
  
  escreva("\nDigite outro número: ")
  leia(num2)

  se (num1 > num2) {
    escreva("\nNúmero maior é: ",num1,".\n")
  } senao {
   escreva("\nNúmero maior é: ",num2,".\n")}
   
  }
}
